{
    "name": "quick-ray-dumper",
    "displayName": "Quick Ray Dumper",
    "description": "Automating the process of writing Ray's syntax to dump stuff",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:quick-ray-dumper.showQueries",
        "onCommand:quick-ray-dumper.showQueriesCallback",
        "onCommand:quick-ray-dumper.stopShowingQueries",
        "onCommand:quick-ray-dumper.countQueries",
        "onCommand:quick-ray-dumper.showDuplicateQueries",
        "onCommand:quick-ray-dumper.stopShowingDuplicateQueries",
        "onCommand:quick-ray-dumper.showDuplicateQueriesCallback",
        "onCommand:quick-ray-dumper.showSlowQueries",
        "onCommand:quick-ray-dumper.showSlowQueriesCallback",
        "onCommand:quick-ray-dumper.showEvents",
        "onCommand:quick-ray-dumper.stopShowingEvents",
        "onCommand:quick-ray-dumper.showEventsCallback",
        "onCommand:quick-ray-dumper.showJobs",
        "onCommand:quick-ray-dumper.stopShowingJobs",
        "onCommand:quick-ray-dumper.showJobsCallback",
        "onCommand:quick-ray-dumper.showCache",
        "onCommand:quick-ray-dumper.stopShowingCache",
        "onCommand:quick-ray-dumper.showHttpClientRequests",
        "onCommand:quick-ray-dumper.stopShowingHttpClientRequests",
        "onCommand:quick-ray-dumper.showHttpClientRequestsCallback",
        "onCommand:quick-ray-dumper.showViews",
        "onCommand:quick-ray-dumper.showEnv",
        "onCommand:quick-ray-dumper.showRequests",
        "onCommand:quick-ray-dumper.model"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "quick-ray-dumper.showQueries",
                "title": "Show Queries"
            },
            {
                "command": "quick-ray-dumper.showQueriesCallback",
                "title": "Show Queries By Callback"
            },
            {
                "command": "quick-ray-dumper.stopShowingQueries",
                "title": "Stop Showing Queries"
            },
            {
                "command": "quick-ray-dumper.countQueries",
                "title": "Count Queries"
            },
            {
                "command": "quick-ray-dumper.showDuplicateQueries",
                "title": "Show Duplicate Queries"
            },
            {
                "command": "quick-ray-dumper.stopShowingDuplicateQueries",
                "title": "Stop Show Duplicated Queries"
            },
            {
                "command": "quick-ray-dumper.showDuplicateQueriesCallback",
                "title": "Show Duplicated Queries By Callback"
            },
            {
                "command": "quick-ray-dumper.showSlowQueries",
                "title": "Show Slow Queries"
            },
            {
                "command": "quick-ray-dumper.showSlowQueriesCallback",
                "title": "Show Slow Queries By Callback"
            },
            {
                "command": "quick-ray-dumper.showEvents",
                "title": "Show Events"
            },
            {
                "command": "quick-ray-dumper.stopShowingEvents",
                "title": "Stop Showing Events"
            },
            {
                "command": "quick-ray-dumper.showEventsCallback",
                "title": "Show Events By Callback"
            },
            {
                "command": "quick-ray-dumper.showJobs",
                "title": "Show Jobs"
            },
            {
                "command": "quick-ray-dumper.stopShowingJobs",
                "title": "Stop Showing Jobs"
            },
            {
                "command": "quick-ray-dumper.showJobsCallback",
                "title": "Show Jobs By Callback"
            },
            {
                "command": "quick-ray-dumper.showCache",
                "title": "Show Cache"
            },
            {
                "command": "quick-ray-dumper.stopShowingCache",
                "title": "Stop Showing Cache"
            },
            {
                "command": "quick-ray-dumper.showHttpClientRequests",
                "title": "Show Http Client Requests"
            },
            {
                "command": "quick-ray-dumper.stopShowingHttpClientRequests",
                "title": "Stop Showing Http Client Requests"
            },
            {
                "command": "quick-ray-dumper.showHttpClientRequestsCallback",
                "title": "Show Http Client Requests By Callback"
            },
            {
                "command": "quick-ray-dumper.showViews",
                "title": "Show Views"
            },
            {
                "command": "quick-ray-dumper.showEnv",
                "title": "Show Env"
            },
            {
                "command": "quick-ray-dumper.showRequests",
                "title": "Show Requests"
            },
            {
                "command": "quick-ray-dumper.model",
                "title": "Show Model"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.63.0",
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "eslint": "^8.1.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.3",
        "typescript": "^4.4.4",
        "ts-loader": "^9.2.5",
        "webpack": "^5.52.1",
        "webpack-cli": "^4.8.0",
        "@vscode/test-electron": "^1.6.2"
    }
}
